
  "{{ name }}" = {
    description                  = ""
    mac_address_table_aging      = "{{ mac_address_table_aging }}"
    mac_aging_time               = 14500
    organization                 = "{{ org }}"
    udld_message_interval        = 15
    udld_recovery_action         = "reset"
    vlan_port_count_optimization = {{ vlan_port_count_optimization|lower }}
{%- if tags %}
    tags = [
  {%- for item in tags %}
      {
    {%- for k, v in item.items() %}
        {{ k }} = "{{ v }}",
    {%- endfor %}
      },
  {%- endfor %}
    ]
{%- else %}
    tags                         = []
{%- endif %}
  }
